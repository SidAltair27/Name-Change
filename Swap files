/* Define the paths to the folders */
%let folder1 = /path/to/folder1;
%let folder2 = /path/to/folder2;

/* List of file names */
%let file_names = file1.sas7bdat file2.sas7bdat file3.sas7bdat /* Add more file names here */;

/* Swap files between folders */
%macro swap_files;
    %do i = 1 %to %sysfunc(countw(&file_names));
        %let file_name = %scan(&file_names, &i);

        proc datasets library=&folder1 nodetails;
            change "&folder1./&file_name" = "&folder2./&file_name";
        quit;

        proc datasets library=&folder2 nodetails;
            change "&folder2./&file_name" = "&folder1./&file_name";
        quit;
    %end;
%mend swap_files;

/* Run the macro to swap the files */
%swap_files;
