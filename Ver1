/* Step 1: Connect to a CAS session */
cas mySession sessopts=(caslib="casuser");

/* Step 2: Define the folder paths */
%let curr_folder = /path/to/curr;
%let prev_folder = /path/to/prev;
%let temp_folder = /path/to/temp;

/* Step 3: Rename folders using CAS actions */
proc cas;
    session mySession;

    /* Rename 'curr' to 'temp' */
    action file.rename /
        caslib="casuser"
        source="&curr_folder"
        destination="&temp_folder";
    
    /* Rename 'prev' to 'curr' */
    action file.rename /
        caslib="casuser"
        source="&prev_folder"
        destination="&curr_folder";

    /* Rename 'temp' to 'prev' */
    action file.rename /
        caslib="casuser"
        source="&temp_folder"
        destination="&prev_folder";
quit;

/* Step 4: Terminate the CAS session */
cas mySession terminate;
